{
    "workspaceRoot": "..",
    "composeFile": "../docker-compose.yml",
    "orchestrator": {
        "script": "../matrix_orchestrator.py",
        "plan": "../plan.json",
        "python": "auto"
    },
    "julia": {
        "healthUrl": "http://localhost:9000/healthz"
    },
    "ai": {
        "provider": "openai-compatible",
        "baseUrl": "http://localhost:8000/v1",
        "apiKeyEnv": "OPENAI_API_KEY",
        "model": "gpt-4o-mini",
        "system": "You are an assistant embedded in the NuRea IDE. When given file context and a selection, produce precise patches and short diffs.",
        "temperature": 0.2
    }
}