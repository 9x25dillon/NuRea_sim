{
  "version": "1.0",
  "run_id": "demo-001",
  "backend": { 
    "julia_url": "http://julia-backend:9000",
    "chaos_rag_url": "http://chaos-rag:8081"
  },
  "pipeline": [
    {
      "op": "load_matrix",
      "source": "data/matrix.csv",
      "shape": [64, 64],
      "seed": 42
    },
    {
      "op": "optimize",
      "method": "chebyshev_projection",
      "sparsity": 0.15,
      "backend": "julia"
    },
    {
      "op": "vectorize",
      "method": "l2_normalize",
      "dimensions": 1536,
      "backend": "chaos_rag"
    },
    {
      "op": "save",
      "target": "artifacts/optimized_matrix.csv"
    }
  ],
  "metadata": {
    "description": "Demo: Matrix optimization with Julia backend + vectorization via ChaosRAGJulia",
    "logging": "info",
    "deterministic": true
  }
}
